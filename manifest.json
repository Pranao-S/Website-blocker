{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
    "action":{       
    "default_popup": "Popup.html",
    "default_title": "Website Blocker"
  },
  "host_permissions": [
    "https://www.netflix.com/*",
    "https://www.primevideo.com/*"
  ]
}
